

/*//////////////////////////////////////////////////////////////////
[ FONT ]*/


@font-face {
  font-family: Poppins-Regular;
  src: url('../fonts/poppins/Poppins-Regular.ttf'); 
}

@font-face {
  font-family: Poppins-Bold;
  src: url('../fonts/poppins/Poppins-Bold.ttf'); 
}

/*//////////////////////////////////////////////////////////////////
[ RESTYLE TAG ]*/
* {
	margin: 0px; 
	padding: 0px; 
	box-sizing: border-box;
}

body, html {
	height: 100%;
	font-family: sans-serif;
}

/* ------------------------------------ */
a {
	margin: 0px;
	transition: all 0.4s;
	-webkit-transition: all 0.4s;
  -o-transition: all 0.4s;
  -moz-transition: all 0.4s;
}

a:focus {
	outline: none !important;
}

a:hover {
	text-decoration: none;
}

/* ------------------------------------ */
h1,h2,h3,h4,h5,h6 {margin: 0px;}

p {margin: 0px;}

ul, li {
	margin: 0px;
	list-style-type: none;
}


/* ------------------------------------ */
input {
  display: block;
	outline: none;
	border: black;
	
}

textarea {
  display: block;
  outline: none;
}

textarea:focus, input:focus {
  border-color: black;
}

/* ------------------------------------ */
button {
	outline: none !important;
	border: none;
	background: black;
}

button:hover {
	cursor: pointer;
}

iframe {
	border: none !important;
}


/*//////////////////////////////////////////////////////////////////
[ Table ]*/

.limiter {
  width: 100%;
  margin: 0 auto;
}

#ign {
  display: block;
}

.container-table100 {
  width: 100%;
  min-height: 100vh;
  background: #c4d3f6;

  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  padding: 33px 30px;
}

.wrap-table100 {
  width: 95%;
  border-radius: 10px;
  overflow: hidden;
}

.table {
  width: 100%;
  display: table;
  margin: 0;
}

/*Alteração Cor Tabela pela prioridade*/

.row {
  display: table-row;
  background: #fff;
}

.row1 {
  display: table-row;
  background: rgb(150, 192, 150);
}

.row2 {
  display: table-row;
  background: rgb(247, 247, 92);
}

.row3 {
  display: table-row;
  background: rgb(255, 80, 80);
}

.row4 {
  display: table-row;
  background: rgb(22, 16, 16);
}

.row.header {
  color: #ffffff;
  background: #6c7ae0;
}




.cell {
  display: table-cell;
}


/*Row0*/
.row .cell {
  font-family: Poppins-Regular;
  font-size: 25px;
  color: #000000;
  line-height: 1.2;
  font-weight: unset !important;

  padding-top: 20px;
  padding-bottom: 20px;
  border-bottom: 1px solid #f2f2f2;
}

.row.header .cell {
  font-family: Poppins-Regular;
  font-size: 25px;
  color: #fff;
  line-height: 1.2;
  font-weight: unset !important;

  padding-top: 19px;
  padding-bottom: 19px;
}

.row .cell:nth-child(1) {
  width: 21%;
  padding-left: 20px;
}

.row .cell:nth-child(2) {
  width: 8%;
}

.row .cell:nth-child(3) {
  width: 14%;
